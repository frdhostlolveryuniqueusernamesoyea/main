{
  "functions": {
    "api/routes/q_and_a_route.js": {
      "runtime": "nodejs14.x"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/white", "status": 308, "dest": "/indexwhite.html" },
    { "src": "/dark", "status": 308, "dest": "/index.html" },
    { "src": "/(.*)", "status": 404, "dest": "/404/index.html" }
  ]
}
